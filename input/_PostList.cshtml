<script>
    const posts = [
        @foreach(var post in Document.GetDocumentList("AllPosts")){
            <text>
                {Link: '@post.Destination', 
                Title: '@post.GetString("Title")',
                Thumbnail: '@post.GetString("Thumbnail")',
                Published: '@post.GetString("Published")',
                Author: '@post.GetString("Author")',
                Tags: [
                    @foreach(var tag in post.GetList<string>("Tags")){
                        <text>
                            '@tag',
                        </text>
                    }
                ]},
            </text>
        }
    ]
</script>

<section class="post_list">
</section>